//Manager.java
import java.io.*;
import java.util.*;

public class Manager
{
  private String name;
  private int pin;
  
  public Manager(String name, int pin)
  {
    this.name = name;
    this.pin = pin;
  }
  
  public String getName()
  {
    return name;
  }
  
  public int getPin()
  {
    return pin;
  }
  
  public String toString()
  {
    return name + " " + pin;
  }
}